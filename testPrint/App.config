<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="watchdog" type="System.Configuration.NameValueSectionHandler" />
    <section name="printers" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <watchdog>
    <add key="Directory" value="C:\Users\user\Downloads" />
  </watchdog>
  <printers>
    <add key="Big_EPL_Label" value="label" />
    <add key="Small_EPL_Label" value="barcode" />
    <add key="Big_ZPL_Label" value="labelZPL" />
    <add key="Small_ZPL_Label" value="barcodeZPL" />
  </printers>
</configuration>